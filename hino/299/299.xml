  <numero>299</numero>
  <titulo>Renovação</titulo>
  <texto>
    <estrofe>
      <verso>Fortalece a tua Igreja,</verso>
      <verso>Ó bendito Salvador!</verso>
      <verso>Dá-lhe tua plena graça,</verso>
      <verso>Oh! Renova seu vigor.</verso>
      <verso>Vivifica, vivifica</verso>
      <verso>Nossas almas, ó Senhor!</verso>
      <verso>Vivifica, vivifica</verso>
      <verso>Nossas almas, ó Senhor! Amém.</verso>
    </estrofe>
  </texto>
